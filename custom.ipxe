#!ipxe
###
### netboot.xyz-custom menu example
###

:custom
clear custom_choice
menu Please choose an option:
item --gap Linux Distros
item endless6    ${space} Endless OS 6 2025.06.10
item pop2204     ${space} Pop OS 22.04
item --gap Utilities:
item hirens      ${space} Hirens Boot CD PE x64 (2025.10.19)
item strelec     ${space} Strelec (2025.09.29)
item --gap iVentoy:
item iventoy     ${space} iVentoy
item iventoy2    ${space} iVentoy2
item --gap Virtualization:
item proxmox9    ${space} Proxmox VE 9.0.1
item --gap Windows:
item win11       ${space} Windows 11 25H2
item --gap --
item reboot      ${space} Reboot
choose custom_choice || goto custom_exit
echo ${cls}
echo ${base-url}
echo ${live_endpoint}
goto ${custom_choice}
goto custom_exit

endless6:
set base-url ${live_endpoint}/ISO/eos-eos6.0-amd64-amd64.250610-092714.en.iso
kernel https://boot.netboot.xyz/memdisk raw iso
initrd ${base-url}
boot || goto custom_exit

:hirens
echo Booting Hirens Boot CD PE x64...
set base-url ${live_endpoint}/hirens
imgfree
kernel https://boot.netboot.xyz/wimboot
initrd -n bootmgr     ${base-url}/BOOTMGR           bootmgr ||
initrd -n bootmgr.efi ${base-url}/bootmgr.efi       bootmgr.efi ||      
initrd -n bcd         ${base-url}/boot/bcd          bcd||
initrd -n boot.sdi    ${base-url}/boot/boot.sdi     boot.sdi||
initrd -n boot.wim    ${base-url}//sources/boot.wim  boot.wim
boot || goto custom_exit

:pop2204
set base-url ${live_endpoint}/ISO/pop-os_22.04_amd64_intel_58.iso
kernel https://boot.netboot.xyz/memdisk raw iso
initrd ${base-url}
boot || goto custom_exit

:proxmox9
set base-url ${live_endpoint}/ISO/proxmox-ve_9.0-1.iso
kernel https://boot.netboot.xyz/memdisk raw iso
initrd ${base-url}
boot || goto custom_exit

:strelec
echo Booting Strelec...
set base-url ${live_endpoint}/ISO/strelec
kernel http://${boot_domain}/wimboot
initrd ${base_url}/bootmgr       bootmgr ||
initrd ${base_url}/BCD           BCD ||
initrd ${base_url}/boot.sdi      boot.sdi ||
initrd ${base_url}/strelec81.wim boot.wim
initrd ${base_url}/bootmgr       bootmgr ||
initrd ${base_url}/BCD           BCD ||
initrd ${base_url}/boot.sdi      boot.sdi ||
initrd ${base_url}/strelec81.wim boot.wim
boot || goto custom_exit

:strelec_iso
set base-url ${live_endpoint}/ISO/WinPE11_10_Sergei_Strelec_x64_2025.09.29_English.iso
kernel https://boot.netboot.xyz/memdisk raw iso
initrd ${base-url}
boot || goto custom_exit

win11:
set base-url ${live_endpoint}/ISO/Win11_25H2_English_x64.iso
kernel https://boot.netboot.xyz/memdisk raw iso
initrd ${base-url}
boot || goto custom_exit

:iventoy
set user-class iVentoy
dhcp
set user-class 
chain tftp://${next-server}/iventoy_loader_16000_uefi

:iventoy2
# set user-class iVentoy
set newserver:ipv4 192.168.199.6
set newbootfile iventoy_loader_16000_uefi
set net0/next-server ${newserver}
set net1/next-server ${newserver}
set net0/filename ${newbootfile}
set net1/filename ${newbootfile}
chain tftp://${newserver}/iventoy_loader_16000

:reboot
reboot

:custom_exit
exit
